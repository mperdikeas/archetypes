                    </source-folder>
                    <source-folder style="packages">
                        <label>cars-war/src/java</label>
                        <location>${project.dir}/cars-war/src/java</location>
                    </source-folder>
                    <source-file>
                        <location>${ant.script}</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="run"/>
                </context-menu>
            </view>
            <subprojects/>
