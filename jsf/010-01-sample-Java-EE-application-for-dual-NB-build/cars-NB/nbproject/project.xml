                    <location>${project.dir}/cars-war/src/java</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <script>${ant.script}</script>
                    <target>ear</target>
                </action>
                <action name="clean">
                    <script>${ant.script}</script>
                    <target>clean</target>
                </action>
                <action name="run">
                    <script>${ant.script}</script>
