{
  "name": "serverside-node-babel-mocha-flow-only-for-testing",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "raze": "rm -fr node_modules",
    "flow-check": "flow check",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flow.README": [
      "If you wish to run flow continuously use the following incantation (from the command line): ",
      "$ watch -n 1 -t -d npm run flow                                                             "
    ],
    "flow-stop": "flow stop",
    "test": "mocha --require source-map-support/register --compilers js:babel-register",
    "test-watch": "mocha --require source-map-support/register --compilers js:babel-register --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.6.1",
    "flow-bin": "^0.35.0",
    "in-publish": "^2.0.0",
    "mocha": "^2.5.3"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "lodash": "^4.10.0",
    "source-map-support": "^0.4.0"
  }
}
